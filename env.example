# Configurações Gerais
NODE_ENV=development

# Admin Frontend
NEXT_PUBLIC_API_URL=http://localhost:3333
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_project_id_here

# Serviço de Autenticação
AUTH_PORT=3333
AUTH_JWT_SECRET=your_jwt_secret_here
AUTH_DB_URL=postgresql://postgres:postgres@localhost:5432/crypto_wallet_auth

# Serviço de Carteira
WALLET_PORT=3334
WALLET_DB_URL=postgresql://postgres:postgres@localhost:5432/crypto_wallet
WALLET_ENCRYPTION_KEY=your_encryption_key_here

# Serviço KYC
KYC_PORT=3335
KYC_DB_URL=postgresql://postgres:postgres@localhost:5432/crypto_wallet_kyc

# Serviço de Liquidez
LIQUIDITY_PORT=3336
LIQUIDITY_DB_URL=postgresql://postgres:postgres@localhost:5432/crypto_wallet_liquidity

# Serviço de Notificação
NOTIFICATION_PORT=3337
NOTIFICATION_DB_URL=postgresql://postgres:postgres@localhost:5432/crypto_wallet_notification

# WhatsApp API
WHATSAPP_API_KEY=your_whatsapp_api_key_here
WHATSAPP_PHONE_NUMBER=your_whatsapp_phone_number_here

# Blockchain
ETHEREUM_RPC_URL=your_ethereum_rpc_url_here
BSC_RPC_URL=your_bsc_rpc_url_here
POLYGON_RPC_URL=your_polygon_rpc_url_here

