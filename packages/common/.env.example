# Configurações da API Notus
NOTUS_API_URL=https://api.notus.com/v1
NOTUS_API_KEY=your_api_key_here
NOTUS_WEBHOOK_URL=https://your-domain.com/api/webhooks/notus
NOTUS_WEBHOOK_SECRET=your_webhook_secret_here

# Rate Limiting
NOTUS_RATE_LIMIT_MESSAGES_PER_MINUTE=60
NOTUS_RATE_LIMIT_WEBHOOKS_PER_SECOND=100

# Templates
NOTUS_TEMPLATE_NAMESPACE=fintech_wallet
NOTUS_TEMPLATE_LANGUAGE=pt_BR

# Monitoring
NOTUS_MONITORING_ENABLED=true
NOTUS_ERROR_LOGGING_LEVEL=warn